{
    "name": "hyphun/hypcart",
    "description": "Laravel Package for shopping cart and checkout page.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev":{
        "laravel/framework": "^10.15.0|^11.0",
        "livewire/livewire": "*",
        "cknow/laravel-money": "*"
    },
    "autoload": {
        "psr-4": {
            "Hyphun\\HypCart\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hyphun Technologies",
            "email": "devs@hyphun.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Hyphun\\HypCart\\HypCartServiceProvider"
            ]
        }
    }
}